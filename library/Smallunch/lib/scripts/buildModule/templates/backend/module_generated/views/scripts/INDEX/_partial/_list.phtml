<table border="0" width="100%" class="admin_list">
<tr>

  <?php foreach ($this->display as $dis):?>
    <th class="admin_list">
      <?php echo (key_exists($dis, $this->labels) ? $this->labels[$dis] : ucwords(str_replace('_', ' ', $dis)))?>
    </th> 
  <?php endforeach;?>

</tr>
<?php foreach ($this->[[moduleNameVar]]s as $[[moduleNameVar]]): ?>
<tr>
  <?php foreach ($this->display as $dis):?>
    <td><a href="<?php echo $this->url(array('module'=>'[[moduleName]]', 'controller'=>'[[CONTROLLER_NAME]]', 'action'=>'edit', 'id'=>$[[moduleNameVar]]['id']));?>"><?php echo $[[moduleNameVar]][$dis];?></a></td>
  <?php endforeach;?>
</tr>
<?php endforeach; ?>
</table>